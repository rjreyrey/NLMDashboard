{
  "name": "nlm-dashboard",
  "version": "0.3.13",
  "description": "NLM Dashboard",
  "main": "./src/main.js",
  "repository": "https://github.com/rjreyrey/NLMDashboard",
  "scripts": {
    "build-css": "node-sass-chokidar assets/ -o assets/",
    "start-js": "electron -r babel-register .",
    "start": "npm-run-all build-css start-js",
    "dist": "npm run build-css && electron-builder",
    "build-js": "build --win",
    "build": "npm-run-all build-css build-js",
    "ship": "npm-run-all build-css ship-js",
    "ship-js": "build --win -p always"
  },
  "author": {
    "name": "Reynolds & Reynolds"
  },
  "devDependencies": {
    "electron": "^1.8.4",
    "electron-builder": "^20.8.1"
  },
  "build": {
    "appId": "com.electron.nlmdashboard",
    "productName": "NLM Dashboard",
    "win": {
      "icon": "assets/icons/win/icon.ico"
    }
  },
  "dependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "electron-is-dev": "^0.3.0",
    "electron-log": "^2.2.14",
    "electron-redux": "^1.3.1",
    "electron-tabs": "^0.9.1",
    "electron-updater": "^2.21.4",
    "node-sass": "^4.8.3",
    "node-sass-chokidar": "^1.2.2",
    "npm-run-all": "^4.1.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "rijndael-js": "^1.0.0"
  },
  "-vs-binding": {
    "BeforeBuild": [
      "build-css"
    ]
  }
}
